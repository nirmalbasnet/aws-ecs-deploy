{
  "family": "aws-ecs-deploy-family",
  "containerDefinitions": [
    {
      "name": "aws-ecs-demo-container",
      "image": "827040639836.dkr.ecr.ap-south-1.amazonaws.com/aws-ecs-deploy-demo:1.0.0",
      "cpu": 0,
      "portMappings": [
        {
          "name": "aws-ecs-demo-container-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/aws-ecs-deploy-family",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "cpu": "512",
  "memory": "1024",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
